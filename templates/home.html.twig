{% extends "base.html.twig" %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset ("styles/home.style.css")}}">
{% endblock %}

{% block body %}

    {# CAROUSEL #}
    <div class="fondBlanc">
        <div id="carouselExampleControls" class="carousel slide contenu" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <div class="container">
                        <div class="row">
                            <div class="col-4">
                                <img height="300" src="medias/Soirée Chuchotée.jpg">
                            </div>
                            <div class="col-4">
                                <img height="300" src="medias/affiche.jpg">
                            </div>
                            <div class="col-4">
                                <img height="300" src="medias/Chris2.bmp">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <div class="container">
                        <div class="row">
                            <div class="col-5">
                                <img height="300" src="medias/The End (détail).jpg">
                            </div>
                            <div class="col-7">
                                <img height="300" src="medias/57 EME RUE BROOKLIN.jpg">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
            </a>
        </div>
    </div>

    <div class="container-fluid">
        <div class="row">
            <div class="col-3">
                {% include "components/sidebar.html.twig" %}
            </div>
            <div class="col-9"></div>
        </div>
    </div>



    {# {% include "components/footer.html.twig" %} #}

{% endblock %}